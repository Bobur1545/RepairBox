{
  "name": "tucker-eric/eloquentfilter",
  "description": "An Eloquent way to filter Eloquent Models",
  "keywords": [
    "laravel",
    "eloquent",
    "model",
    "search",
    "query",
    "filter"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Eric Tucker",
      "email": "tucker.ericm@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/console": "~6.0|~7.0|~8.0",
    "illuminate/filesystem": "~6.0|~7.0|~8.0",
    "illuminate/database": "~6.0|~7.0|~8.0",
    "illuminate/support": "~6.0|~7.0|~8.0",
    "illuminate/config": "~6.0|~7.0|~8.0",
    "illuminate/pagination": "~6.0|~7.0|~8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "EloquentFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "EloquentFilter\\TestClass\\": "tests/classes"
    }
  },
  "config": {
        "preferred-install": "dist"
    },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
        "providers": [
            "EloquentFilter\\ServiceProvider"
        ]
    }
  }
}
